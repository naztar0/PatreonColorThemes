{
  "exceptionStyles": {
    "header > :nth-child(2) > div": {
      "background-color": "var(--global-bg-base-default)"
    },
    "header > :nth-child(2) :where(span, a)": {
      "color": "var(--global-content-regular-default)"
    },
    "header .active ~ .header-container": {
      "color": "var(--global-content-regular-default)"
    },
    "header .active ~ .header-container span": {
      "color": "var(--global-content-regular-default)",
      "opacity": "1"
    },
    "header .active ~ .header-container svg": {
      "fill": "var(--global-content-regular-default)"
    },
    "header .active ~ .header-container :where(.nav-link, div[data-label=\"Pricing\"]):before": {
      "display": "none"
    },
    "header .active ~ .header-container :where(.nav-link, div[data-label=\"Pricing\"]):after": {
      "display": "none"
    }
  }
}
