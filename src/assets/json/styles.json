{
  "rootStyleVars": [
    "--global-bg-page-default",
    "--global-bg-pageAlt-default",
    "--global-bg-base-default",
    "--global-bg-base-hover",
    "--global-bg-base-pressed",
    "--global-bg-baseAlt-default",
    "--global-bg-baseAlt-hover",
    "--global-bg-baseAlt-pressed",
    "--global-bg-elevated-default",
    "--global-bg-elevated-hover",
    "--global-bg-elevated-pressed",
    "--global-content-regular-default",
    "--global-content-regular-hover",
    "--global-content-regular-pressed",
    "--global-content-muted-default",
    "--global-content-muted-hover",
    "--global-content-muted-pressed",
    "--global-content-inverted-default",
    "--global-content-inverted-hover",
    "--global-content-inverted-pressed",
    "--global-content-invertedMuted-default",
    "--global-content-invertedMuted-hover",
    "--global-content-invertedMuted-pressed",
    "--global-primary-action-default",
    "--global-primary-action-hover",
    "--global-primary-action-pressed",
    "--global-primary-onAction-default",
    "--global-primary-onAction-hover",
    "--global-primary-onAction-pressed",
    "--global-primary-muted-default",
    "--global-primary-muted-hover",
    "--global-primary-muted-pressed",
    "--global-primary-onMuted-default",
    "--global-primary-onMuted-hover",
    "--global-primary-onMuted-pressed",
    "--global-secondary-action-default",
    "--global-secondary-action-hover",
    "--global-secondary-action-pressed",
    "--global-secondary-onAction-default",
    "--global-secondary-onAction-hover",
    "--global-secondary-onAction-pressed",
    "--global-secondary-muted-default",
    "--global-secondary-muted-hover",
    "--global-secondary-muted-pressed",
    "--global-secondary-onMuted-default",
    "--global-secondary-onMuted-hover",
    "--global-secondary-onMuted-pressed",
    "--global-success-action-default",
    "--global-success-action-hover",
    "--global-success-action-pressed",
    "--global-constant-black-default",
    "--global-constant-white-default"
  ],
  "additionalStyles": {
    ".pct-animate *": {
      "transition": "background-color 0.2s ease-in-out"
    },
    "mark": {
      "background-color": "var(--global-bg-base-default)",
      "color": "var(--global-content-regular-default)"
    },
    "input:not([name=\"color\"])": {
      "background": "none",
      "color": "var(--global-content-regular-default)"
    },
    "button[data-tag=\"file-upload-button\"]": {
      "color": "var(--global-content-regular-default)",
      "background-color": "var(--global-bg-elevated-default)"
    },
    "button[data-tag=\"file-upload-button\"] svg": {
      "fill": "var(--global-content-regular-default)"
    },
    "div[aria-label=\"Resource Center\"]": {
      "background-color": "var(--global-bg-base-default)",
      "box-shadow": "var(--global-boxShadow-mid)"
    },
    ".bb-text, #pendo-base div": {
      "background-color": "var(--global-bg-base-default)",
      "color": "var(--global-content-regular-default)"
    },
    "span[style*=\"color: rgb(0, 0, 0)\"]": {
      "color": "var(--global-content-regular-default)"
    },
    "div[data-tag=\"launch-banner\"] > div > div": {
      "background-color": "var(--global-bg-baseAlt-default)"
    },
    "div[color=\"var(--global-bg-base-default)\"]": {
      "background-image": "linear-gradient(transparent 0%, var(--global-bg-base-default) 100%)"
    }
  },
  "exceptionRoot": [
    "--global-border-action-hover: var(--global-primary-muted-default);",
    "--global-border-action-default: var(--global-primary-muted-hover);"
  ],
  "overriders": [
    "main > div:first-child",
    "main > div:first-child > div:first-child",
    "main > div:nth-of-type(2)",
    ".reactWrapper > div:first-child",
    "@div[data-tag=\"dropdown-list\"]",
    "@div[role=\"dialog\"]!div[data-tag=\"account-menu\"]",
    "@div[id^=\"Popover\"]!div",
    "@div[data-focus-guard=\"true\"]!.reactWrapper,#Popover-0 > div"
  ]
}
